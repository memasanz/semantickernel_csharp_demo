
-- Drop existing objects
DROP VIEW IF EXISTS vw_UnitAvailability;
DROP TABLE IF EXISTS IncidentNotes;
DROP TABLE IF EXISTS UnitCapabilities;
DROP TABLE IF EXISTS Units;
DROP TABLE IF EXISTS Incidents;
DROP TABLE IF EXISTS Capabilities;

-- Create tables
CREATE TABLE Capabilities (
    CapabilityCode NVARCHAR(10) PRIMARY KEY,
    Description NVARCHAR(100)
);

CREATE TABLE Incidents (
    IncidentKey UNIQUEIDENTIFIER PRIMARY KEY,
    IncidentId NVARCHAR(50),
    Latitude FLOAT,
    Longitude FLOAT,
    Timestamp DATETIME,
    Status NVARCHAR(10) CHECK (Status IN ('Open', 'Closed'))
);

CREATE TABLE Units (
    UnitKey UNIQUEIDENTIFIER PRIMARY KEY,
    UnitId NVARCHAR(50),
    AssignedArea NVARCHAR(100),
    Available BIT,
    Status NVARCHAR(20),
    LastKnownLatitude FLOAT,
    LastKnownLongitude FLOAT,
    AssignedIncident UNIQUEIDENTIFIER NULL,
    FOREIGN KEY (AssignedIncident) REFERENCES Incidents(IncidentKey)
);

CREATE TABLE UnitCapabilities (
    UnitKey UNIQUEIDENTIFIER,
    CapabilityCode NVARCHAR(10),
    PRIMARY KEY (UnitKey, CapabilityCode),
    FOREIGN KEY (UnitKey) REFERENCES Units(UnitKey),
    FOREIGN KEY (CapabilityCode) REFERENCES Capabilities(CapabilityCode)
);

CREATE TABLE IncidentNotes (
    NoteId UNIQUEIDENTIFIER PRIMARY KEY,
    IncidentKey UNIQUEIDENTIFIER,
    Timestamp DATETIME,
    NoteText NVARCHAR(500),
    FOREIGN KEY (IncidentKey) REFERENCES Incidents(IncidentKey)
);

