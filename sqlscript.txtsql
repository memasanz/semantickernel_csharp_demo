-- Create view
CREATE VIEW vw_UnitAvailability AS
SELECT 
    u.UnitId,
    u.AssignedArea,
    u.Available,
    u.Status,
    u.LastKnownLatitude,
    u.LastKnownLongitude,
    STRING_AGG(uc.CapabilityCode, ',') AS Capabilities,
    i.IncidentId AS AssignedIncidentId,
    i.Timestamp AS IncidentTimestamp,
    i.Latitude AS IncidentLatitude,
    i.Longitude AS IncidentLongitude,
    i.Status AS IncidentStatus
FROM Units u
LEFT JOIN UnitCapabilities uc ON u.UnitKey = uc.UnitKey
LEFT JOIN Incidents i ON u.AssignedIncident = i.IncidentKey
GROUP BY u.UnitId, u.AssignedArea, u.Available, u.Status, u.LastKnownLatitude, u.LastKnownLongitude, i.IncidentId, i.Timestamp, i.Latitude, i.Longitude, i.Status;
